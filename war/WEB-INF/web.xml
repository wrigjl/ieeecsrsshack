<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>ACMDLUpdater</servlet-name>
		<servlet-class>net.thought.ieeecsrsshack.ACMUpdaterServlet</servlet-class>
	</servlet><servlet>
		<servlet-name>IEEECSJournal</servlet-name>
		<servlet-class>net.thought.ieeecsrsshack.IEEECSJournalServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>IEEECSConference</servlet-name>
		<servlet-class>net.thought.ieeecsrsshack.IEEECSConferenceServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>IEEECSJournal</servlet-name>
		<url-pattern>/ieeecsjournal</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>IEEECSConference</servlet-name>
		<url-pattern>/ieeecsconference</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
	<servlet-mapping>
		<servlet-name>ACMDLUpdater</servlet-name>
		<url-pattern>/acmdlupdater</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ACMDisplay</servlet-name>
		<url-pattern>/acmdisplay</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ACMDisplay</servlet-name>
		<servlet-class>net.thought.ieeecsrsshack.ACMDisplayServlet</servlet-class>
	</servlet>
	<security-constraint>
		<web-resource-collection>
			<web-resource-name>updater</web-resource-name>
			<url-pattern>/acmdlupdater</url-pattern>
		</web-resource-collection>
		<auth-constraint>
			<role-name>admin</role-name></auth-constraint>
	</security-constraint>
</web-app>
